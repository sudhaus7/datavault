{
    "name": "SUDHAUS7/guard7",
    "description": "(Sudhaus7) Plugin and framework for encrypting and decrypting data in Typo3, inkluding a RSA public and private key management.",
    "version": "8.0.0",
    "type": "typo3-cms-extension",
    "authors": [
        {
            "name": "Frank Berger",
            "role": "Developer",
            "email": "fberger@sudhaus7.de"
        },
        {
            "name": "Markus Hofmann",
            "role": "Developer",
            "email": "mhofmann@sudhaus7.de"
        }
    ],
    "require": {
        "php": "~7.0.0 || ~7.1.0 || ~7.2.0",
        "typo3/cms-core": "^8.7.0"
    },
    "require-dev": {
        "typo3-console/php-server-command": "^0.1.1",
        "nimut/testing-framework": "^4.1",
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "^0.11",
        "codeception/codeception": "^3.1",
        "codeception/phpbuiltinserver": "^1.5"
    },
    "replace": {
        "guard7": "self.version"
    },
    "autoload": {
        "psr-4": {
            "SUDHAUS7\\Guard7\\": "Classes/"
        }
    }
}
